{
  "name": "Print to Boox",
  "version": "0.0.8",
  "description": "Print any page to Boox reader (via boox push service)",
  "options_page": "options.html",
  "default_locale": "en_GB",
  "background": {
    "scripts": ["aliyun-oss-sdk-6.0.0.min.js", "analytics.js", "error.js", "background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Print to Boox",
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "permissions": [ "*://send2boox.com/", "*://push.boox.com/", "printerProvider", "notifications", "storage", "webRequest" ],
  
  "manifest_version": 2
}
